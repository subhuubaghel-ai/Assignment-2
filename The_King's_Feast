import java.util.*;
public class KingFeast{
    public static void main(String[] args){
        int n = 5;
        int[] arr = {2, 7, 1, 9, 5};
        int max = findMax(arr);
        System.out.println(max);
    }

    public static int findMax(int[] arr){
        int max = arr[0];
        for(int i = 1; i < arr.length; i++){
            if(arr[i] > max){
                max = arr[i];
            }
        }
        return max;
    }
}
